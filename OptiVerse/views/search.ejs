<!DOCTYPE html>
<html>
<head>
    <title>Search Results</title>
</head>
<body>
    <% if (posts.length > 0) { %>
        <% posts.forEach(function(post) { %>
            <div class="feed-container">
                <div class="feed-box">
                    <p class="user-info"><%= post.username %> | <%= post.date %></p>
                    <p class="post-info"><%= post.post %></p>
                    <div class="feed-interactions">
                        <a class="feed-btn"><i class="fa-regular fa-heart fa-2x"></i></a>
                        <a class="feed-btn"><i class="fa-solid fa-share fa-2x"></i></a>
                    </div>
                </div>
            </div>
        <% }); %>
    <% } else { %>
        <p>No results found for "<%= query %>".</p>
    <% } %>
</body>
</html>
